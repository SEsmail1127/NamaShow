@model NamaShow.Core.VeiwModels.Movie.Session.CreateSessionForAdmin

@{
    ViewData["Title"] = "Create";
}


<div class="main-panel">
    <div class="content-wrapper">
        <div class="row">
            <div class="col-md-8 grid-margin stretch-card">
                <div class="card">
                    <div class="card-body">
                        <h4 class="card-title btn btn-outline-primary btn-rounded btn-fw"> <a class="text-dark " href="~/Admin/Sessio/Index?id=@Model.MovieId">Index</a></h4>
                        <h4 class="card-title">Create Session</h4>
                        <form asp-action="Create" method="post" class="forms-sample" >
                            <div class="form-group">
                                <label for="exampleInputName1">LinkName</label>
                                <input type="text" class="form-control" asp-for="SeasonNumber" placeholder="SeasonNumber">
                                <span class="validate-color" asp-validation-for="SeasonNumber"></span>
                            </div>
                            <input type="hidden" asp-for="MovieId" />
                            <button class="btn btn-success mr-2">Submit</button>
                        </form>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>


@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
